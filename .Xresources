! my customisations
xterm*faceName: Inconsolata
xterm*faceSize: 14
xterm*termName: xterm-256color

! Base16 Summerfruit
! Scheme: Christopher Corley (http://cscorley.github.io/)

#define base00 #151515
#define base01 #202020
#define base02 #303030
#define base03 #505050
#define base04 #B0B0B0
#define base05 #D0D0D0
#define base06 #E0E0E0
#define base07 #FFFFFF
#define base08 #FF0086
#define base09 #FD8900
#define base0A #ABA800
#define base0B #00C918
#define base0C #1faaaa
#define base0D #3777E6
#define base0E #AD00A1
#define base0F #cc6633

xterm*foreground:   base02
xterm*background:   base07
xterm*cursorColor:  base02

xterm*color0:       base00	! black
xterm*color1:       base08	! red
xterm*color2:       base0B	! green
xterm*color3:       base0A	! yellow
xterm*color4:       base0D	! blue
xterm*color5:       base0E	! magenta
xterm*color6:       base0C	! cyan
xterm*color7:       base05	! white

xterm*color8:       base03	! black
xterm*color9:       base08	! red
xterm*color10:      base0B	! green
xterm*color11:      base0A	! yellow
xterm*color12:      base0D	! blue
xterm*color13:      base0E	! magenta
xterm*color14:      base0C	! cyan
xterm*color15:      base07	! white

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
xterm*color16:      base09
xterm*color17:      base0F
xterm*color18:      base01
xterm*color19:      base02
xterm*color20:      base04
xterm*color21:      base06

